{
  "name": "backend",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Eduardo Andrade <eduardo.andrade15@hotmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node-dev --transpile-only --ignore-watch node_modules ./src/server.ts",
    "start": "ts-node-dev --transpile-only --ignore-watch node_modules ./src/server.ts",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config src/config/ormconfig.ts",
    "migration:create": "yarn typeorm migration:create -n",
    "migration:run": "yarn typeorm migration:run",
    "migration:revert": "yarn typeorm migration:revert",
    "test": "jest --runInBand"
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "http-status": "^1.5.0",
    "jsonwebtoken": "^8.5.1",
    "mysql2": "^2.2.5",
    "nodemon": "^2.0.7",
    "swagger-jsdoc": "^6.0.1",
    "swagger-ui-express": "^4.1.6",
    "typeorm": "^0.2.30",
    "typeorm-naming-strategies": "^2.0.0",
    "yup": "^0.32.8"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.11",
    "@types/http-status": "^1.1.2",
    "@types/jest": "^26.0.23",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/supertest": "^2.0.11",
    "@types/swagger-jsdoc": "^6.0.0",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/yup": "^0.29.11",
    "jest": "^27.0.2",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.1",
    "ts-node-dev": "^1.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.3"
  }
}
